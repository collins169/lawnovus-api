{
  "name": "lawnovus-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "author": "Collins Onyetube",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "start": "concurrently --kill-others-on-fail yarn:start:services yarn:build yarn:start:sls:local",
    "start:sls:local": "node bin/wait-for-local-postgres.js && yarn typeorm migration:run && sls offline start --stage local",
    "start:sls:dev": "sls offline start --stage dev",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "build:watch": "chokidar 'src/**/*.{ts,json}' -c 'yarn build && echo finished compiling' --initial",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --collectCoverage=false --detectOpenHandles --unhandled-rejections=strict",
    "test:coverage": "yarn test --coverage",
    "sls:print": "sls print",
    "sls:create:cert": "sls create-cert",
    "sls:deploy": "sls deploy",
    "sls:destroy": "sls remove",
    "lint:check": "concurrently yarn:prettier:check yarn:eslint:check yarn:build:check",
    "lint:staged": "lint-staged",
    "eslint:check": "eslint . --ext .ts,.js",
    "eslint:fix": "eslint . --fix --ext .ts,.js",
    "prettier:check": "prettier --check 'src/**/*'",
    "prettier:fix": "prettier --write 'src/**/*'",
    "start:services": "docker-compose up",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js --dataSource migration.config.ts",
    "typeorm:seed": "ts-node-dev ./node_modules/typeorm-seeding/dist/cli.js seed --configName ormconfig.local --dataSource migration.config.ts",
    "migration:generate": "yarn typeorm migration:generate -p ./src/database/migrations/$NAME",
    "migration:run": "yarn typeorm migration:run"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.102",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/luxon": "^3.3.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.7.13",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.0.1",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.2.2",
    "msw": "^1.2.1",
    "prettier": "2.8.8",
    "serverless": "^3.22.0",
    "serverless-certificate-creator": "^1.6.0",
    "serverless-domain-manager": "^7.1.1",
    "serverless-hooks-plugin": "^1.1.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.2",
    "serverless-prune-plugin": "^2.0.2",
    "serverless-s3-local": "^0.7.2",
    "serverless-webpack": "^5.13.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4",
    "wait-on": "^7.0.1",
    "webpack": "^5.94.0",
    "webpack-node-externals": "^3.0.0",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.332.0",
    "@aws-sdk/client-lambda": "^3.329.0",
    "@aws-sdk/client-s3": "^3.329.0",
    "@aws-sdk/client-ssm": "^3.329.0",
    "@aws-sdk/s3-request-presigner": "^3.338.0",
    "@nestjs/common": "^9.4.0",
    "@types/bcryptjs": "^2.4.2",
    "aws-lambda": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.19.2",
    "helmet": "^7.0.0",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.0",
    "lambda-multipart-parser": "^1.0.1",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "mime-types": "^2.1.35",
    "pg": "^8.10.0",
    "serverless-http": "^3.2.0",
    "standard": "^17.0.0",
    "typeorm": "^0.3.15",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-cloudwatch": "^6.1.1"
  }
}
